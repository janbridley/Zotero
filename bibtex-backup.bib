@article{palomares-baezDecmonNewNanoparticle2019,
  title = {The {{Decmon}}: A New Nanoparticle Shape along the Truncation Path from the Icosahedron to the Decahedron},
  shorttitle = {The {{Decmon}}},
  author = {{Palomares-B{\'a}ez}, Juan Pedro and {Montejano-Carrizales}, Juan Mart{\'i}n and Guisbiers, Gr{\'e}gory and {Jos{\'e}-Yacam{\'a}n}, Miguel and {Rodr{\'i}guez-L{\'o}pez}, Jos{\'e} Luis},
