@article{panTargetedDiscoveryLowCoordinated2023,
  title = {Targeted {{Discovery}} of {{Low-Coordinated Crystal Structures}} via {{Tunable Particle Interactions}}},
  author = {Pan, Hillary and Dshemuchadse, Julia},
  year = {2023},
  month = apr,
  journal = {ACS Nano},
  volume = {17},
  number = {8},
  pages = {7157--7169},
