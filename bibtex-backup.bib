@article{dejagerPhaseBehaviorCrystal2024,
  title = {Phase Behavior and Crystal Nucleation of Hard Triangular Prisms},
  author = {De Jager, Marjolein and Slaats, Nena and Filion, Laura},
  year = {2024},
  month = oct,
  journal = {The Journal of Chemical Physics},
  volume = {161},
  number = {13},
