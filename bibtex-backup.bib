@article{abidSynthesisCharacterizationCO22016,
  title = {Synthesis, Characterization, and {{CO2}} Adsorption of Three Metal-Organic Frameworks ({{MOFs}}): {{MIL-53}}, {{MIL-96}}, and Amino-{{MIL-53}}},
  shorttitle = {Synthesis, Characterization, and {{CO2}} Adsorption of Three Metal-Organic Frameworks ({{MOFs}})},
  author = {Abid, Hussein Rasool and Rada, Zana Hassan and Shang, Jin and Wang, Shaobin},
  year = {2016},
  month = dec,
  journal = {Polyhedron},
  volume = {120},
  pages = {103--111},
  issn = {02775387},
  doi = {10.1016/j.poly.2016.06.034},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0277538716302686},
  urldate = {2023-10-31},
  abstract = {In this study, MIL-53, MIL-96, and amino-MIL-53 were prepared, characterized, and tested for CO2 adsorption. These metal-organic frameworks (MOFs) exhibit different characteristics, although MIL-53 and amino-MIL-53 have the same topology. The BET surface areas are 1519, 687, and 262 m2/g for MIL-53, MIL-96, and amino-MIL-53, respectively. They exhibit different thermal stability with MIL-53 having the highest stability which starts to decompose at 773 K, while amino-MIL-53 and MIL-96 show lower thermal stability, decomposing upon heating up to 650 and 570 K, respectively. Static adsorption of CO2 at 1 bar and 273 K was conducted, showing CO2 adsorption capacities of 64, 124, and 48 cc/g for MIL53, MIL-96, and amino-MIL-53, respectively. The heat of adsorption for CO2 was found to be 39, 28.6, and 28 kJ/mol for MIL-53, MIL-96, and amino-MIL-53, respectively. Dynamic adsorption experiment shows that MIL-53 achieves the highest working capacity among all three materials around 169 cc/g at 1 bar and room temperature (304 K). Amino-MIL-53 shows a dynamic adsorption capacity of 121 cc/g at the same conditions and MIL-96 demonstrates a dynamic adsorption of 98.2 cc/g at 1 bar and 298 K. The higher working capacity demonstrated by MIL-53 and amino-MIL-53 are attributed to their larger pore size, making them promising candidate adsorbents for practicing carbon capture in real-world applications.},
  langid = {english},
  file = {/Users/jenna/Zotero/storage/9ECNM4P8/Abid et al. - 2016 - Synthesis, characterization, and CO2 adsorption of three metal-organic frameworks (MOFs) MIL-53, MI.pdf}
}

