@article{stelmarShallowdepthVaginoplastyPreoperative2023,
  title = {Shallow-Depth Vaginoplasty: Preoperative Goals, Postoperative Satisfaction, and Why Shallow-Depth Vaginoplasty Should Be Offered as a Standard Feminizing Genital Gender-Affirming Surgery Option},
  shorttitle = {Shallow-Depth Vaginoplasty},
  author = {Stelmar, Jenna and Smith, Shannon M. and Lee, Grace and Zaliznyak, Michael and Garcia, Maurice M.},
  year = {2023},
  month = oct,
  journal = {The Journal of Sexual Medicine},
  volume = {20},
