@article{wangShapeControlledSynthesisPalladium2013,
  title = {Shape-{{Controlled Synthesis}} of {{Palladium Nanocrystals}}: {{A Mechanistic Understanding}} of the {{Evolution}} from {{Octahedrons}} to {{Tetrahedrons}}},
