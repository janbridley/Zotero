@article{coliArtificialNeuralNetwork2021,
  title = {An {{Artificial Neural Network Reveals}} the {{Nucleation Mechanism}} of a {{Binary Colloidal AB}}{\textsubscript{13}} {{Crystal}}},
  author = {Coli, Gabriele M. and Dijkstra, Marjolein},
  year = {2021},
