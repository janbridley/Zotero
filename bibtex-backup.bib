@article{orofinoRelationshipSecondVirial1957,
  title = {Relationship of the {{Second Virial Coefficient}} to {{Polymer Chain Dimensions}} and {{Interaction Parameters}}},
  author = {Orofino, T. A. and Flory, P. J.},
  year = {1957},
  month = may,
  journal = {The Journal of Chemical Physics},
  volume = {26},
  number = {5},
